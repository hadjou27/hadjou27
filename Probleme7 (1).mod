int n=...;
 int p=...;
range I=1..n;
range E=1..n;
 int c[1..8][1..8]=...;
  

dvar boolean X[I][E];
//dvar int c[I][I];

maximize  sum(i in E,j in I) c[i][j]*X[i][j];
//maximize sum(i,j in I) c[i][j]*x[i][j];


subject to{
 forall(i in I){
  forall(j in I){
     X[i][j]==X[j][i] ; 
  }}
X[1][2]+X[1][4]+X[1][5] <= 1;
  X[1][2]+X[2][3]+X[2][4]+X[2][6] <= 1;
     X[3][2]+X[3][6]+X[3][7] <= 1;
     X[4][1]+X[4][2]+X[5][4]+X[4][6] <= 1;
     
     X[5][1]+X[5][4]+X[5][6]+X[5][8] <= 1;
     X[6][2]+X[6][3]+X[6][4]+X[5][6]+X[6][7]+X[6][8] <= 1;
     X[7][3]+X[7][6]+X[7][8] <= 1;
     X[7][8]+X[6][8]+X[5][8]  <= 1;
     }